        "gatsby-plugin-sitemap": "^4.1.0",
        "gatsby-plugin-ts-config": "^1.1.5",
        "gatsby-plugin-typescript": "^3.5.0",
        "husky": "^6.0.0",
        "lint-staged": "^11.0.0",
        "postcss-preset-env": "^6.7.0",
        "prettier": "^2.3.0",
        "sass": "^1.33.0",
        "typescript": "^4.2.4",
        "typescript-plugin-css-modules": "^3.2.0"
