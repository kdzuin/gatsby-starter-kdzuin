{
    "name": "gatsby-starter-kdzuin",
    "version": "1.0.0",
    "private": true,
    "description": "Gatsby Starter with TypeScript, SCSS + CSS Modules, Base Layout and Typography",
    "author": "Konstantin Dzuin",
    "keywords": [
        "gatsby"
    ],
    "license": "0BSD",
    "scripts": {
